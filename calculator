#include <iostream>
using namespace std;
int main(){
    int numAgents;
    double agentSalary;
    int chatInteractions;
    int voiceInteractions;
    int resolutionTime;

    cout<< "How many agents do you have employed?"<<endl;
    cin >> numAgents;
    cout << "What is the monthly Salary of these agents?"<< endl;
    cin >> agentSalary;
    cout<< "How many chat interactions do you have per month?"<< endl;
    cin>> chatInteractions
    cout<< "How many voice interactions do you have per month?"<< endl;
    cin>> voiceInteractions
    cout<< "What is your average resolution time?"<< endl;
    cin >> resolutionTime
    int totalInteractions;
    totalInteractions= chatInteractions+voiceInteractions;

    int liscensingFee;
    liscensingFee= 24000;
    int deliveryFee;
    deliveryFee= 7000;

  double costPerInteraction;

  if (totalInteractions>=100000){
    costPerInteraction=.012;
  }
  else if (totalInteractions>=50000){
    costPerInteraction=.016;
  }
  else if (totalInteractions>=10000){
    costPerInteraction=.02;
  }
  else if (totalInteractions>=0){
    costPerInteraction=.024;
  }
  else{
    cout<<"Invalid interactions";
      }

  double aiCost;
  aiCost= ((chatInteractions+voiceInteractions)*.3*12)*costPerInteraction);
  double voiceTechnology;
  voiceTechnology= (((voiceInteractions*resolutionTime)*.3*.5)/60)*2.4);

  double totalAI;
  totalAI= voiceTechnology+aiCost+liscensingFee+deliveryFee;

  double normalCost;
  normalCost=numAgents*agentSalary;
  double newTotal;
  newTotal= totalAI+(normalCost*.7);

  double profit;

  profit= newTotal-normalCost;

  cout<< "Your profit would be"<< profit<< endl;

return 0
   
}
